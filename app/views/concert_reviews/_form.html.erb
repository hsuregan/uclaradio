

<%= form_for @concertreview do |f| %>
  <div class="control-group">
   
   <div class="col-lg-6">

    <%= f.label :Title %>
    <div class="controls">
    <%= f.text_field :title, class: "form-control" %>
    </div>

    <%= f.label :Critic %>
    <div class="controls">
    <%= f.text_field :author, class: "form-control" %>
    </div>

    <%= f.label :Venue %>
    <div class="controls">
    <%= f.text_field :location, class: "form-control" %>
    </div>

    <%= f.label :email %>
    <div class="controls">
    <%= f.text_field :email, class: "form-control" %>
    </div>
  

    <%= f.label :artist %>
    <div class="controls">
      <input class = "form-control" id = "artists" <%= text_field(:artist, :name) %>
    </div>
    

    <%= f.label :image %> URL
    <div class="controls">
    <%= f.text_field :image, class: "form-control" %>
    </div>
  
   </div>

  <div class="col-lg-6">
    <div class="control-group">
      <%= f.label :content %>
      <div class="controls">
        <%= f.text_area :content, rows: "22", cols: "50", class: "form-control" %>
      </div>
    </div>
  </div>


  <div class="row">
    <div class="col-lg-12">
    <div class="form-actions">
    <br />
    <center>
    <%= f.submit nil, class: "btn btn-primary" %>
    <%= link_to 'Cancel', concert_reviews_path, class: "btn btn-primary" %>
    </center>
    </div>
    </div>





  </div>




<% end %>


